#JSGF V1.0;

/**
 * JSGF Grammar for dialog manager demo
 */

grammar email;
import <commands.standard_commands>;

public <commands> = <standard_commands> ;

public <command> =      read message             |
                        delete message           |
                        reply to message         |
                        compose new message      |
                        next   message           |
                        previous message;
