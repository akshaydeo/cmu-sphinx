# This script runs a particular test in the given directory
#

base=$PWD
log=$base/regression.log

directory=$1
target=$2
rawOutput=$target.out
alignOutput=$target.align

cd $directory
if gmake $target > $rawOutput
then
  status="OK";
else
  status="FAILED";
fi

$base/s4_align $rawOutput > $alignOutput
$base/parseLog $rawOutput $alignOutput $status >>  $log
