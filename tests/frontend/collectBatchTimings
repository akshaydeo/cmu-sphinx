#!/bin/sh

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtests.frontend.PreemphasizerTest.dumpTimes=true \
 -Dtests.frontend.PreemphasizerTest.batchMode=true \
 tests.frontend.PreemphasizerTest \
 PreemphasizerTest \
 frontend.props \
 batch.ctl

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtests.frontend.HammingWindowerTest.dumpTimes=true \
 -Dtests.frontend.HammingWindowerTest.batchMode=true \
 tests.frontend.HammingWindowerTest \
 HammingWindowerTest \
 frontend.props \
 batch.ctl

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtests.frontend.SpectrumAnalyzerTest.dumpTimes=true \
 -Dtests.frontend.SpectrumAnalyzerTest.batchMode=true \
 tests.frontend.SpectrumAnalyzerTest \
 SpectrumAnalyzerTest \
 frontend.props \
 batch.ctl

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtests.frontend.CmnFeatureExtractorTest.dumpTimes=true \
 tests.frontend.CmnFeatureExtractorTest \
 cepstra.batch.ctl
