#!/bin/sh

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtest.frontend.PreemphasizerTest.dumpTimes=true \
 -Dtest.frontend.PreemphasizerTest.batchMode=true \
 tests.frontend.PreemphasizerTest batch.ctl

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtests.frontend.HammingWindowerTest.dumpTimes=true \
 -Dtests.frontend.HammingWindowerTest.batchMode=true \
 tests.frontend.HammingWindowerTest batch.ctl

${JAVA_HOME}/bin/java -server -cp ../../classes \
 -Dtests.frontend.CmnFeatureExtractorTest.dumpTimes=true \
 tests.frontend.CmnFeatureExtractorTest cepstra.batch.ctl
