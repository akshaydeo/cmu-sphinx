############################################################
# Global logging properties
############################################################
handlers= java.util.logging.ConsoleHandler

# This is the overall default logging level
.level= SEVERE

# Tracing level for the console
java.util.logging.ConsoleHandler.level = ALL
java.util.logging.ConsoleHandler.formatter = edu.cmu.sphinx.util.SphinxLogFormatter

edu.cmu.sphinx.util.LogMath.logBase = 1.0001


############################################################
# 
# Search properties
#
############################################################

edu.cmu.sphinx.linguist.dictionary.Dictionary.dictionaryPath = ./FSTGrammarTest.dic
edu.cmu.sphinx.linguist.dictionary.Dictionary.wordReplacement = pittsburgh
edu.cmu.sphinx.linguist.dictionary.Dictionary.fillerPath = ./filler.dic
edu.cmu.sphinx.linguist.language.grammar.FSTGrammar.path=./FSTGrammarTest.gram
